 <servlet-class>signup</servlet-class>
 </servlet> 
  
 <servlet-mapping>
 <servlet-name>ba</servlet-name>
 <url-pattern>/sign</url-pattern>
 </servlet-mapping> 
 
 <servlet>
 <servlet-name>bc</servlet-name>
 <servlet-class>ForgetPassword</servlet-class>
 </servlet> 
  
 <servlet-mapping>
 <servlet-name>bc</servlet-name>
 <url-pattern>/forgot</url-pattern>
 </servlet-mapping> 
 
 <servlet>
 <servlet-name>cb</servlet-name>
 <servlet-class>ChangePassword</servlet-class>
 </servlet> 
  
 <servlet-mapping>
 <servlet-name>cb</servlet-name>
 <url-pattern>/change</url-pattern>
 </servlet-mapping> 
  
  </web-app>